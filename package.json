    "@babel/runtime": "^7.13.10",
    "mobile-detect": "^1.4.5",
    "react-responsive": "^8.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/modosc/react-responsive-redux.git"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
